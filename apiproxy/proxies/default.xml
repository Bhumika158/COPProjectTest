<?xml version='1.0' encoding='utf-8'?>
<ProxyEndpoint name="default">
    <Flows>
        <Flow name="bp">
            <Description />
            <Request />
            <Response />
            <Condition>(proxy.pathsuffix MatchesPath "/api/bp1") and (request.verb="GET")</Condition>
        </Flow>
        </Flows>
    <HTTPProxyConnection>
        <BasePath>/bp/saltremediation/system</BasePath>
        <Properties />
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
</ProxyEndpoint>